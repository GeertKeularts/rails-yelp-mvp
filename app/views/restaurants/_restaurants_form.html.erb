<%= form_for @restaurant do |f| %>
<%= f.label :name %>
<%= f.text_field :name %>
<br>
<%= f.label 'phone_number' %>
<%= f.text_field :phone_number %>
<br>
<%= f.label :address %>
<%= f.text_field :address %>
<br>
<%= f.label :category %>
<%= f.select :category, options_for_select( ["chinese", "italian", "japanese", "french", "belgian"])  %>
<br>
<br>
<span class="button"><%= f.submit "add a restaurant"%></span>

<% end %>